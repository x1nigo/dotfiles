#!/bin/sh
alias sr="cd $HOME/.local/src && ls -A" \
h="cd $HOME" \
D="cd $HOME/dls && ls -A" \
d="cd $HOME/dox && ls -A" \
px="cd $HOME/pix && ls -A" \
dt="cd $HOME/.local/share && ls -A" \
sc="cd $HOME/.local/bin && ls -A" \
db="cd $HOME/.local/bin/statusbar && ls -A" \
cfx="$EDITOR $HOME/.config/x11/xresources" \
cfa="$EDITOR $HOME/.config/shell/aliasrc" \
cfs="$EDITOR $HOME/.config/shell/shortcutrc" \
cfd="$EDITOR $HOME/.config/dunst/dunstrc" \
cfz="$EDITOR $HOME/.config/zsh/.zshrc" \
cfZ="$EDITOR $HOME/.config/zathura/zathurarc" \
cfp="$EDITOR $HOME/.config/picom.conf" \
cfn="$EDITOR $HOME/.config/newsboat/config" \
cfnu="$EDITOR $HOME/.config/newsboat/urls" \
cfv="$EDITOR $HOME/.config/nvim/init.vim" \
cfb="$EDITOR $HOME/.local/share/bookmarks" \

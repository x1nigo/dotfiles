set selection-clipboard clipboard
#set recolor
set recolor-lightcolor "#121618"
set recolor-darkcolor "#b7b7b7"
set default-bg "#121618"
set default-fg "#b7b7b7"
set page-padding 1
set highlight-color blue
set highlight-fg black
map r rotate
map R reload
map c recolor
map p print
map a adjust_window best-fit
map s adjust_window width
map g goto top
map G goto bottom

set selection-clipboard clipboard
#set recolor
set recolor-lightcolor "#080808"
set recolor-darkcolor "#808080"
set default-bg "#080808"
set default-fg "#808080"
set page-padding 1
set highlight-color blue
set highlight-fg black
map r rotate
map R reload
map c recolor
map p print
map a adjust_window best-fit
map s adjust_window width
map g goto top
map G goto bottom
